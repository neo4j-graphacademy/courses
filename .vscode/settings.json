{
  "editor.formatOnSave": true,
  "[cypher]": {
    "editor.formatOnSave": false
  },
  "[python]": {
    "editor.formatOnSave": false
  },
  "[asciidoc]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "other": "off",
      "comments": "off",
      "strings": "off"
    }
  },
  "grammarly.selectors": [
    {
      "language": "asciidoc",
      "scheme": "file"
    }
  ],
  "asciidoc.antora.showEnableAntoraPrompt": false,
  "asciidoc.preview.asciidoctorAttributes": {
    "allow-uri-read": true,
  }
}