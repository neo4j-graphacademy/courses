classDef forest fill:#edf6e8,stroke:#b7df9c,stroke-width:1.25px,color:#2f5d1e;
classDef muted fill:#ffffff,stroke:#e5e7eb,stroke-width:1px,color:#334155;
linkStyle default stroke:#94a3b8,stroke-width:1.25px;

graph LR
    subgraph "Graph Model"
        S((Supplier)):::forest
        P((Product)):::forest
        S -->|SUPPLIES| P
    end

    subgraph "Relational Model"
        ST[suppliers table<br/>supplier_id PK]:::muted
        PT[products table<br/>product_id PK<br/>supplier_id FK]:::muted
        ST -.->|referenced by| PT
    end
