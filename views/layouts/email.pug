  style(type='text/css').
    #outlook a {
    padding: 0;
    }
    body {
    margin: 0;
    padding: 0;
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%;
    line-height: 1.8;
    }
    table,
    td {
    border-collapse: collapse;
    mso-table-lspace: 0pt;
    mso-table-rspace: 0pt;
    }
    img {
    border: 0;
    height: auto;
    line-height: 100%;
    outline: none;
    text-decoration: none;
    -ms-interpolation-mode: bicubic;
    }
    p {
    display: block;
    margin: 13px 0;
    }
    a {
      font-weight: bold;
      color: #0056B3;
    }
    table {
      width: 100%;
    }

    td.halign-right {
      text-align: right;
    }
    td.halign-center {
      text-align: center;
    }
    .btn {display: inline-block;background: #006fd6;color: white; padding: 8px 18px; border-radius: 6px; text-decoration: none}
    h2 {
      font-size: 1.2rem;
    }
