div.section(class=[node.$attr('style'),node.$attr('role'),node.$attr('class')].filter(a => !!a).join(' '))
  if node.getTitle()
    if node.level == 1
      h2(id=node.getId()) !{node.getTitle()}
    else if node.level == 2
      h3(id=node.getId()) !{node.getTitle()}
    else if node.level == 3
      h4(id=node.getId()) !{node.getTitle()}
  | !{node.getContent()}
